<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewNombre_"
        android:layout_width="197dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="#F8F9FA"
        android:padding="8dp"
        android:text="Nombre"
        android:textColor="#8C8787"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewTipo"
        android:layout_width="84dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="#F8F9FA"
        android:padding="8dp"
        android:text="Tipo"
        android:textColor="#8C8787"
        app:layout_constraintStart_toEndOf="@+id/textViewNombre_"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_edit"
        android:layout_width="68dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="#F8F9FA"
        android:padding="8dp"
        android:text="Edit"
        android:textColor="#8C8787"
        app:layout_constraintStart_toEndOf="@+id/textViewTipo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_elim"
        android:layout_width="67dp"
        android:layout_height="32dp"
        android:layout_marginTop="8dp"
        android:background="#F8F9FA"
        android:padding="8dp"
        android:text="Elim"
        android:textColor="#8C8787"
        app:layout_constraintStart_toEndOf="@+id/textView_edit"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
